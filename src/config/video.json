{
  "codecs": {
    "x264": {
      "label": "x264",
      "video": {
        "encoder": "libx264",
        "extension": "mp4",
        "settings": {
          "input": [],
          "output": ["-profile", "high", "-tune", "animation", "-movflags", "+faststart"],
          "low": ["-preset", "veryfast", "-crf", 23],
          "medium": ["-preset", "medium", "-crf", 20],
          "high": ["-preset", "slow", "-crf", 18]
        }
      },
      "audio": {
        "encoder": "aac",
        "extension": "aac",
        "settings": ["-b:a", "192k"]
      }
    },
    "webm": {
      "label": "WebM",
      "video": {
        "encoder": "libvpx",
        "extension": "webm",
        "settings": {
          "input": [],
          "output": ["-quality", "good", "-cpu-used", 0, "-qmin", 0, "-qmax", 50, "-b:v", "20M"],
          "low": ["-crf", 10],
          "medium": ["-crf", 5],
          "high": ["-crf", 4]
        }
      },
      "audio": {
        "encoder": "ogg",
        "extension": "ogg",
        "settings": ["-qscale:a", 6]
      }
    },
    "nvenc": {
      "label": "NVEnc",
      "video": {
        "encoder": "h264_nvenc",
        "extension": "mp4",
        "settings": {
          "input": [
            "-threads",
            1,
            "-hwaccel",
            "nvdec",
            "-hwaccel_device",
            0,
            "-hwaccel_output_format",
            "cuda"
          ],
          "output": [
            "-movflags",
            "+faststart"
          ],
          "low": [
            "-tune",
            "ll",
            "-preset",
            "p3",
            "-profile",
            "high",
            "-rc",
            "constqp",
            "-qp",
            23
          ],
          "medium": [
            "-tune",
            "hq",
            "-preset",
            "p4",
            "-profile",
            "high",
            "-rc",
            "constqp",
            "-qp",
            20
          ],
          "high": [
            "-tune",
            "hq",
            "-preset",
            "p6",
            "-profile",
            "high",
            "-rc",
            "constqp",
            "-qp",
            18
          ]
        }
      },
      "audio": {
        "encoder": "aac",
        "extension": "aac",
        "settings": ["-b:a", "192k"]
      }
    }
  }
}
